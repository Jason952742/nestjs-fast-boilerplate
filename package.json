{
  "name": "nestjs-fast-boilerplate",
  "version": "1.0.0",
  "description": "NestJS(Fastify) + MikroORM (MySQL)",
  "author": "陈泊谕 <jason@jaasun.com>",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:e2e": "APIURL=http://localhost:3000/api ./e2e/run-api-tests.sh"
  },
  "renovate": {
    "extends": [
      "config:base",
      ":preserveSemverRanges",
      ":semanticCommitTypeAll(chore)"
    ],
    "semanticCommits": "enabled",
    "separateMajorMinor": false,
    "dependencyDashboard": false,
    "lockFileMaintenance": {
      "enabled": true,
      "schedule": [
        "before 2am"
      ],
      "automerge": true,
      "automergeType": "branch"
    },
    "packageRules": [
      {
        "matchUpdateTypes": [
          "patch",
          "minor"
        ],
        "groupName": "patch/minor dependencies",
        "groupSlug": "all-non-major",
        "automerge": true,
        "automergeType": "branch"
      }
    ],
    "schedule": [
      "every weekday"
    ]
  },
  "dependencies": {
    "@fastify/compress": "^6.5.0",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^6.12.0",
    "@grpc/grpc-js": "^1.9.9",
    "@grpc/proto-loader": "^0.7.10",
    "@mikro-orm/core": "^6.0.0-rc.0",
    "@mikro-orm/entity-generator": "^6.0.0-rc.0",
    "@mikro-orm/migrations": "^6.0.0-rc.0",
    "@mikro-orm/mysql": "^6.0.0-rc.0",
    "@mikro-orm/nestjs": "^5.2.2",
    "@mikro-orm/reflection": "^6.0.0-rc.0",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "10.2.8",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.2.8",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-fastify": "^10.2.8",
    "@nestjs/platform-socket.io": "^10.2.8",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.15",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.8",
    "@socket.io/redis-adapter": "^8.2.1",
    "axios": "^1.6.1",
    "bcryptjs": "2.4.3",
    "bull": "^4.11.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cli-color": "^2.0.3",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "meilisearch": "^0.35.0",
    "mime-types": "^2.1.35",
    "ms": "2.1.3",
    "multer": "1.4.4",
    "multer-s3": "3.0.1",
    "nest-winston": "^1.9.4",
    "nestjs-i18n": "10.3.7",
    "nestjs-meilisearch": "^2.1.5",
    "nodemailer": "6.9.7",
    "passport": "^0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "^7.8.1",
    "slug": "^8.2.2",
    "socket.io": "^4.7.2",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.0.0-rc.0",
    "@mikro-orm/seeder": "^6.0.0-rc.0",
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.2.8",
    "@types/bcryptjs": "2.4.6",
    "@types/bull": "^4.10.0",
    "@types/cli-color": "^2.0.5",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.20",
    "@types/jest": "29.5.8",
    "@types/lodash": "^4.14.201",
    "@types/mime-types": "^2.1.4",
    "@types/ms": "0.7.34",
    "@types/multer": "1.4.10",
    "@types/node": "^20.8.9",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "2.0.16",
    "@types/twitter": "1.7.4",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "atob": ">=2.1.2",
    "deep-extend": ">=0.6.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "extend": ">=3.0.2",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/config/mikro-orm.config.ts",
      "./dist/config/mikro-orm.config.js"
    ]
  },
  "packageManager": "yarn@1.22.21",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
